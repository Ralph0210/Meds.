{
  "expo": {
    "name": "Meds.",
    "slug": "mobile",
    "scheme": "medtracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon-1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0c0e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ralph0210.medtracker",
      "buildNumber": "6",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b0c0e"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ralph0210.medtracker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2ab25341-d0d9-4680-88ad-19251011d58b"
      }
    }
  }
}
